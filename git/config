[alias]
  s = status
  cm = commit -m
  send = push
  update = "!f() { git pull origin $(git branch --show-current); }; f"
	cb = "!f() { git branch --show-current; }; f"
  cam = "!f() { \
		git add . && git commit -m \"$1\"; \
	}; f"
  co = checkout
  cob = "!f() { git checkout \"$1\" && git pull origin \"$1\" && git checkout -b \"$2\" \"$1\" && git push --set-upstream origin \"$2\"; }; f"
