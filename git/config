[alias]
  s = status
  cm = commit -m
  send = push
  update = pull
  cam = "!f() { \
		git add . && git commit -m \"$1\"; \
	}; f"
  co = checkout
  cob = "!f() { git checkout \"$1\" && git pull origin \"$1\" && git checkout -b \"$2\" \"$1\"; }; f"
